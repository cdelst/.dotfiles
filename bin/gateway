#!/bin/bash
#Generated by JetBrains Toolbox 2.5.1.34629 at 2024-11-21T10:12:04

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/cdelst/Applications/Gateway.app/Contents/MacOS/gateway" $wait --args "${intellij_args[@]}"