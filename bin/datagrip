#!/bin/bash
#Generated by JetBrains Toolbox 2.5.2.35332 at 2025-02-03T23:08:57

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/cdelst/Applications/DataGrip.app/Contents/MacOS/datagrip" $wait --args "${intellij_args[@]}"